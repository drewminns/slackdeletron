{
  "name": "slackdeletron",
  "version": "3.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "8.10.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie 10"
  ],
  "jest": {
    "collectCoverageFrom": [
      "client/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|svg)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "./config/setupTests.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run lint",
      "pre-push": "npm test && npm run lint"
    }
  },
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "prod": "npm run lint && npm test && npm run asset-build",
    "dev": "NODE_ENV=development nodemon index.js --ignore client",
    "asset-build": "npm run clean && NODE_ENV=production webpack-cli --config config/webpack.config.js",
    "test": "NODE_ENV=test jest",
    "heroku-postbuild": "npm run asset-build",
    "lint": "eslint server/** client/** routes/**",
    "clean": "rimraf dist"
  },
  "keywords": [],
  "author": "Drew Minns",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^8.1.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "classcat": "^3.0.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "css-loader": "^0.28.10",
    "eslint": "^4.19.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "express": "^4.16.2",
    "express-http-to-https": "^1.1.4",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.22.0",
    "node-sass": "^7.0.0",
    "normalize.css": "^8.0.0",
    "postcss-loader": "^2.1.1",
    "prop-types": "^15.6.1",
    "raven-js": "^3.24.1",
    "react": "^16.3.0",
    "react-dates": "^16.4.0",
    "react-dom": "^16.3.0",
    "react-transition-group": "^2.3.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "sw-precache-webpack-plugin": "^0.11.5",
